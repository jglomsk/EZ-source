make basketeer do wp single_basket
	make reverse_print do wp length
		while length > 0
			show single_basket[length - 1]
		done
	done
	make print_basket do wp length
		set y to 0
		while y < length
			show single_basket[y]
		done
	done
done
set i to basket
push hello into i
push 82 into i
call basketeer wp i
set x to 2
call print_basket wp x
